{
  "name": "ilenia",
  "version": "9.9.0",
  "description": "Components and utilities for translations and localization in React",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "keywords": [
    "react",
    "localization",
    "translate",
    "translation"
  ],
  "homepage": "https://github.com/trustpilot/ilenia",
  "repository": {
    "type": "git",
    "url": "https://github.com/trustpilot/ilenia.git"
  },
  "bugs": {
    "url": "https://github.com/trustpilot/ilenia/issues"
  },
  "author": "Trustpilot",
  "peerDependencies": {
    "prop-types": "^15",
    "react": "^16.8"
  },
  "dependencies": {
    "htmlstring-to-react": "^5.2.1",
    "javascript-time-ago": "^2.3.6"
  },
  "scripts": {
    "build": "rimraf dist && tsc",
    "lint": "eslint --ext .ts --ext .tsx .",
    "lint:fix": "npm run lint -- --fix",
    "start": "jest --watch",
    "test": "is-ci \"test:ci\" \"test:watch\"",
    "test:ci": "jest",
    "test:watch": "jest --watch"
  },
  "jest": {
    "preset": "ts-jest",
    "modulePathIgnorePatterns": [
      "<rootDir>/dist"
    ]
  },
  "devDependencies": {
    "@testing-library/react": "^11.2.2",
    "@trustpilot/semanticrelease-config": "^2.0.2",
    "@types/jest": "^26.0.15",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^1.11.0",
    "@typescript-eslint/parser": "^1.11.0",
    "babel-eslint": "^10.1.0",
    "commitlint": "^11.0.0",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-trustpilot": "^4.5.1",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-html": "^6.1.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^1.6.1",
    "husky": "^4.3.0",
    "is-ci-cli": "1.1.1",
    "jest": "^26.6.3",
    "prettier": "^1.18.2",
    "prop-types": "^15.6.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.4",
    "typescript": "^3.9.7"
  },
  "husky": {
    "hooks": {
      "commit-msg": "npx commitlint -E HUSKY_GIT_PARAMS --extends @commitlint/config-conventional"
    }
  }
}
