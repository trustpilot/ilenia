{
  "name": "ilenia",
  "version": "8.2.0",
  "description": "Components and utilities for translations and localization in React",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "keywords": [
    "react",
    "localization",
    "translate",
    "translation"
  ],
  "homepage": "https://github.com/trustpilot/ilenia",
  "repository": {
    "type": "git",
    "url": "https://github.com/trustpilot/ilenia.git"
  },
  "bugs": {
    "url": "https://github.com/trustpilot/ilenia/issues"
  },
  "author": "Trustpilot",
  "peerDependencies": {
    "prop-types": "15.x",
    "react": "^16.3.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.3.1",
    "htmlstring-to-react": "4.0.1",
    "timeago.js": "3.0.2"
  },
  "scripts": {
    "build": "babel src --out-dir dist --copy-files --ignore __tests__",
    "lint": "eslint src/",
    "test": "NODE_ENV=test jest"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "verbose": true,
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/jestSetup.js"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.2",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@semantic-release/changelog": "^2.1.1",
    "@semantic-release/git": "^6.0.1",
    "@semantic-release/npm": "^3.4.0",
    "@semantic-release/release-notes-generator": "^6.1.1",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "commitlint": "^7.0.0",
    "conventional-changelog-eslint": "^3.0.0",
    "eslint": "^4.19.1",
    "eslint-config-trustpilot": "^4.1.2",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-html": "^5.0.3",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.0.2",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "prettier": "^1.16.4",
    "prop-types": "^15.6.1",
    "raf": "^3.4.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.3",
    "react-test-renderer": "^16.3.1",
    "react-testing-library": "^5.8.0",
    "semantic-release": "^15.7.2",
    "transform-runtime": "^0.0.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
