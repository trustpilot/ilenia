{
  "name": "ilenia",
  "version": "9.7.0",
  "description": "Components and utilities for translations and localization in React",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "keywords": [
    "react",
    "localization",
    "translate",
    "translation"
  ],
  "homepage": "https://github.com/trustpilot/ilenia",
  "repository": {
    "type": "git",
    "url": "https://github.com/trustpilot/ilenia.git"
  },
  "bugs": {
    "url": "https://github.com/trustpilot/ilenia/issues"
  },
  "author": "Trustpilot",
  "peerDependencies": {
    "prop-types": "^15",
    "react": "^16.8"
  },
  "dependencies": {
    "htmlstring-to-react": "^5.2.0",
    "javascript-time-ago": "^2.0.13"
  },
  "scripts": {
    "build": "rimraf dist && tsc",
    "lint": "eslint --ext .ts --ext .tsx .",
    "lint:fix": "npm run lint -- --fix",
    "start": "jest --watch",
    "test": "is-ci \"test:ci\" \"test:watch\"",
    "test:ci": "jest",
    "test:watch": "jest --watch"
  },
  "jest": {
    "preset": "ts-jest",
    "setupFilesAfterEnv": [
      "@testing-library/react/cleanup-after-each"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/dist"
    ]
  },
  "devDependencies": {
    "@semantic-release/changelog": "^3.0.4",
    "@semantic-release/git": "^7.0.16",
    "@semantic-release/npm": "^5.1.13",
    "@semantic-release/release-notes-generator": "^7.2.1",
    "@testing-library/react": "^8.0.4",
    "@types/jest": "^24.0.15",
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^1.11.0",
    "@typescript-eslint/parser": "^1.11.0",
    "babel-eslint": "^10.1.0",
    "commitlint": "^9.1.2",
    "conventional-changelog-eslint": "^3.0.8",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-trustpilot": "^4.5.1",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-html": "^6.0.2",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.5",
    "eslint-plugin-react-hooks": "^1.6.1",
    "husky": "^3.0.0",
    "is-ci-cli": "1.1.1",
    "jest": "^26.2.0",
    "prettier": "^1.18.2",
    "prop-types": "^15.6.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "rimraf": "2.6.3",
    "semantic-release": "^15.13.27",
    "ts-jest": "^26.1.4",
    "typescript": "^3.9.7"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
