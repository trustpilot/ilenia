{
  "compact": true,
  "presets": [
    [
      "babel-preset-env",
      {
        "loose": false,
        "modules": false
      }
    ],
    "babel-preset-react"
  ],
  "plugins": [
    [
      "transform-runtime",
      {
        "helpers": false,
        "polyfill": false,
        "regenerator": true
      }
    ],
    "transform-object-rest-spread",
    "transform-react-jsx"
  ],
  "env": {
    "test": {
      "plugins": ["transform-es2015-modules-commonjs"]
    }
  }
}
